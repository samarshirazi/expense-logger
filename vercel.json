{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/build",
  "functions": {
    "server/index.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
